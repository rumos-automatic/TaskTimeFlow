{
  "name": "@tasktimeflow/web",
  "version": "1.0.0",
  "description": "TaskTimeFlow Web Application - Next.js Frontend",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build", 
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules",
    "analyze": "ANALYZE=true npm run build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build",
    "preview": "next build && next start",
    "db:generate": "supabase gen types typescript --project-id \"$NEXT_PUBLIC_SUPABASE_PROJECT_ID\" --schema public > lib/database.types.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "studio": "supabase studio",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lighthouse": "npx lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "bundle:analyze": "cross-env ANALYZE=true next build",
    "bundle:visualizer": "npx webpack-bundle-analyzer .next/static/chunks/*.js"
  },
